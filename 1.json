{
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "http.proxySupport": "off",
    "code-runner.runInTerminal": true,
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "code-runner.executorMap": {
        "cpp": " cd $dir && g++ $fileName -D LOCAL -o D:/MyFile/exe/$fileNameWithoutExt.exe && D:/MyFile/exe/$fileNameWithoutExt.exe",
        "c": " cd $dir && g++ $fileName -D LOCAL -o D:/MyFile/exe/$fileNameWithoutExt.exe && D:/MyFile/exe/$fileNameWithoutExt.exe",
    },
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "git.confirmSync": false,
    "python.insidersChannel": "off",
    "files.exclude": {
        "**/__pycache__": true
    },
    "explorer.confirmDragAndDrop": false,
    "vsicons.dontShowNewVersionMessage": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmDelete": false,
    "python.pythonPath": "D:\\Program Files\\Python\\Python38\\python.exe",
    "window.doubleClickIconToClose": true,
    "window.titleBarStyle": "custom",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.semanticHighlighting.enabled": true,
    "python.languageServer": "Pylance",
    "git.ignoreMissingGitWarning": true,
    "python.dataScience.alwaysTrustNotebooks": true,
    "code-runner.executorMapByFileExtension": {
        ".vb": "cd $dir && vbc /nologo $fileName && $dir$fileNameWithoutExt",
        ".vbs": "cscript //Nologo",
        ".scala": "scala",
        ".jl": "julia",
        ".cr": "crystal",
        ".ml": "ocaml",
        ".exs": "elixir",
        ".hx": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        ".rkt": "racket",
        ".scm": "csi -script",
        ".ahk": "autohotkey",
        ".au3": "autoit3",
        ".kt": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
        ".kts": "kotlinc -script",
        ".dart": "dart",
        ".pas": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        ".pp": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        ".d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        ".hs": "runhaskell",
        ".nim": "nim compile --verbosity:0 --hints:off --run",
        ".csproj": "dotnet run --project",
        ".fsproj": "dotnet run --project",
        ".lisp": "sbcl --script",
        ".kit": "kitc --run",
        ".v": "v run",
        ".vsh": "v run",
        ".sass": "sass --style expanded",
        ".cu": "cd $dir && nvcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
    },
    "backgroundCover.imagePath": "d:\\MyFile\\Pictures\\macOS Big Sur 版本11.0  内置原生壁纸\\Appearance Dynamic3.jpg",
    "backgroundCover.opacity": 1,
    "backgroundCover.randomImageFolder": "d:\\MyFile\\Pictures\\macOS Big Sur 版本11.0  内置原生壁纸",
    "backgroundCover.autoStatus": true,
    "background.enabled": false,
    "workbench.colorTheme": "Abyss",
    "files.autoGuessEncoding": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "[cpp]": {
        "files.encoding": "gbk"
    },
    "[c]": {
        "files.encoding": "gbk"
    }
}